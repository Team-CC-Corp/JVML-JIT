jcd = fs.combine("", shell.getRunningProgram():sub(1, -1 - #(fs.getName(shell.getRunningProgram()))))
jcp = fs.combine(jcd, "CCLib")..":"..shell.dir()
dofile(fs.combine(jcd, "jvml_data/vm/classpath.lua"))
dofile(fs.combine(jcd, "jvml_data/vm/classloader.lua"))
print(loadJavaClass(fs.combine(jcd, "MyFirstProgram.class")))

findMethod(classByName("MyFirstProgram"),"main([Ljava/lang/String;)V")[1]()
